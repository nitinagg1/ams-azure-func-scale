{
  "version": "2.0",
  "extensions": {
        "queues": {
            "batchSize": 32,
            "maxDequeueCount": 1,
            "newBatchThreshold": 16,
            "messageEncoding": "base64"
        }
    },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}